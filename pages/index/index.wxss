view {
  color: var(--themeColor);
}
.index-swiper {
  width: 100%;
}
.index-swiper swiper {
  width: 750rpx;
  height: 320rpx;
}
.index-swiper swiper image {
  width: 100%;
}
.index-navs {
  width: 100%;
  display: flex;
}
.index-navs .nav-item {
  padding: 10rpx;
  flex: 1;
}
.index-navs .nav-item image {
  width: 100%;
}
.index-goods .goods-obj-item .goodsObjTitle {
  width: 100%;
}
.index-goods .goods-obj-item .goodsList {
  overflow: hidden;
  margin-bottom: 10rpx;
}
.index-goods .goods-obj-item .goodsList .goods {
  float: left;
  width: 33.33%;
}
.index-goods .goods-obj-item .goodsList .goods:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10px solid white;
}
.index-goods .goods-obj-item .goodsList .goods:nth-child(2),
.index-goods .goods-obj-item .goodsList .goods:nth-child(3) {
  border-bottom: 10px solid white;
}
.index-goods .goods-obj-item .goodsList .goods image {
  width: 100%;
  height: 100%;
}
