.goods_list .goods_list-content {
  width: 100%;
}
.goods_list .goods_list-content .tabs {
  width: 100%;
  height: 74rpx;
}
.goods_list .goods_list-content .tabs .tabList {
  width: 100%;
  display: flex;
}
.goods_list .goods_list-content .tabs .tabList .tabs-item {
  flex: 1;
  text-align: center;
  line-height: 74rpx;
}
.goods_list .goods_list-content .tabs .tabList .active-tab {
  border-bottom: 5rpx solid red;
}
